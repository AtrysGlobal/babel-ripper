{
  "name": "@atrysglobal/babel-ripper",
  "version": "1.0.4",
  "description": "Conjunto de herramientas de interfaz y tipado estricto para acceder a servicio de traducciones unificado BABEL.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint && npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "pre-push": [
    "preversion"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtrysGlobal/babel-ripper.git"
  },
  "keywords": [
    "babel",
    "translations",
    "integration",
    "atrys"
  ],
  "author": "Atrys Health.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AtrysGlobal/babel-ripper/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/AtrysGlobal/babel-ripper#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "pre-push": "^0.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "axios": "^0.26.1"
  }
}
